extends layout 
block header 
  include back_header.pug
block content 
  h1= title 

  ul
    each estimate_received in estimate_received_list 
      li  
        a(href="/estimate/received/" + estimate_received._id) #{estimate_received.topic.name} #{estimate_received.user.username}

  hr
  
  if (message)
    h3= message
